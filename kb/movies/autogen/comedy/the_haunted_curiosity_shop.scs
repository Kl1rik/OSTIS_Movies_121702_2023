the_haunted_curiosity_shop <- concept_comedy;

=> nrel_main_idtf:
	[the_haunted_curiosity_shop](*<-lang_en;;<-name_en;;*);
	[__](*<-lang_ru;;<-name_ru;;*);

=> nrel_rating :...(*
    <- concept_rating;;
    => nrel_main_idtf:
       [Rating PG-13](*<-lang_ru;;*);
       [Rating PG-13](*<-lang_en;;*);;
*);

=> nrel_duration_of_showing :...(*
    <- concept_time_showing;;
    => nrel_main_idtf:
       [PT0H2M](*<-lang_ru;;*);
       [PT0H2M](*<-lang_en;;*);;
*);

=> nrel_release_date :...(*
    <- concept_calendar_date;;
    => nrel_main_idtf:
       [1901](*<-lang_ru;;*);
       [1901](*<-lang_en;;*);;
*);;
