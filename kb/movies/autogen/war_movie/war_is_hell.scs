war_is_hell <- concept_war_movie;

=> nrel_main_idtf:
	[war_is_hell](*<-lang_en;;<-name_en;;*);
	[maudite_soit_la_guerre](*<-lang_ru;;<-name_ru;;*);

=> nrel_rating :...(*
    <- concept_rating;;
    => nrel_main_idtf:
       [Rating PG-13](*<-lang_ru;;*);
       [Rating PG-13](*<-lang_en;;*);;
*);

=> nrel_duration_of_showing :...(*
    <- concept_time_showing;;
    => nrel_main_idtf:
       [PT0H50M](*<-lang_ru;;*);
       [PT0H50M](*<-lang_en;;*);;
*);

=> nrel_release_date :...(*
    <- concept_calendar_date;;
    => nrel_main_idtf:
       [1914](*<-lang_ru;;*);
       [1914](*<-lang_en;;*);;
*);;
