the_warrens_of_virginia <- concept_war_movie;

=> nrel_main_idtf:
	[the_warrens_of_virginia](*<-lang_en;;<-name_en;;*);
	[the_warrens_of_virginia](*<-lang_ru;;<-name_ru;;*);

=> nrel_rating :...(*
    <- concept_rating;;
    => nrel_main_idtf:
       [Rating PG-13](*<-lang_ru;;*);
       [Rating PG-13](*<-lang_en;;*);;
*);

=> nrel_duration_of_showing :...(*
    <- concept_time_showing;;
    => nrel_main_idtf:
       [PT0H50M](*<-lang_ru;;*);
       [PT0H50M](*<-lang_en;;*);;
*);

=> nrel_release_date :...(*
    <- concept_calendar_date;;
    => nrel_main_idtf:
       [1915](*<-lang_ru;;*);
       [1915](*<-lang_en;;*);;
*);;
